    "api",
    "nodejs",
    "rest",
    "postgresql",
    "prisma",
    "faker",
    "tsup",
    "tsx",
    "crud",
    "rest-api"
  ],
  "author": "Emiliano Dantas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/themilho/product_catalog/issues"
  },
  "homepage": "https://github.com/themilho/product_catalog#readme",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
